{
  "name": "egg-umi-ssr-demo",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "npm run build && egg-scripts start --port=7001 --daemon --title=egg-umi-ssr --env=prod",
    "stop": "egg-scripts stop --title=egg-umi-ssr",
    "dev": "egg-bin dev --port=7001 --sticky",
    "debug": "cross-env RM_TMPDIR=none COMPRESS=none egg-bin debug",
    "now-build": "npm run prerender",
    "build": "cross-env UMI_ENV=prod umi build",
    "prerender": "npm run start && cross-env UMI_ENV=prerender umi build && egg-scripts stop",
    "umi:dev": "umi dev",
    "test": "egg-bin test",
    "gh-pages": "cross-env GITHUB_PAGE=true npm run prerender && cross-env GITHUB_PAGE=true UMI_ENV=prerender umi gh-pages",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "dependencies": {
    "antd": "^3.19.5",
    "cross-env": "^5.2.0",
    "dva": "^2.6.0-beta.6",
    "egg": "^2.12.0",
    "egg-mysql": "^3.0.0",
    "egg-router-plus": "^1.3.1",
    "egg-scripts": "^2.10.0",
    "egg-sequelize": "^5.2.1",
    "egg-view-assets": "^1.3.0",
    "egg-view-nunjucks": "^2.2.0",
    "moment": "^2.24.0",
    "mysql2": "^2.1.0",
    "prop-types": "^15.6.2",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-sticky": "^6.0.3",
    "sequelize": "^5.21.5",
    "ssr-polyfill": "^1.0.0",
    "umi-request": "1.2.4-beta.1",
    "umi-server": "^1.0.0",
    "umi-utils": "^1.5.4",
    "url": "^0.11.0",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "@types/qs": "^6.5.3",
    "@umijs/fabric": "^1.1.2",
    "@umijs/plugin-prerender": "^1.0.0",
    "antd": "^3.19.5",
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "babel-plugin-dva-hmr": "^0.4.0",
    "babel-plugin-import": "^1.12.0",
    "egg-bin": "^4.9.0",
    "egg-mock": "^3.20.1",
    "eslint": "^5.8.0",
    "eslint-config-egg": "^7.1.0",
    "redbox-react": "^1.5.0",
    "sequelize-cli": "^5.5.1",
    "umi": "^2.12.5",
    "umi-plugin-ecma5-validator": "^0.1.0",
    "umi-plugin-gh-pages": "^0.2.0",
    "umi-plugin-react": "^1.9.0",
    "umi-types": "^0.3.12",
    "umi-utils": "^1.5.4",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
